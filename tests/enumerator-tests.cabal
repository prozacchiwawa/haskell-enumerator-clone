name: enumerator-tests
version: 0
build-type: Simple
cabal-version: >= 1.6

executable enumerator_tests
  main-is: Tests.hs
  ghc-options: -Wall -O2

  build-depends:
      base > 3 && < 5
    , bytestring
    , chell >= 0.1 && < 0.2
    , chell-quickcheck >= 0.1 && < 0.2
    , enumerator
    , QuickCheck
    , split
    , text
    , transformers
